{
    "name": "@hookun/parse-animation-shorthand",
    "publishConfig": {
        "access": "public"
    },
    "description": "A parser for the value of CSS animation shorthand property.",
    "repository": {
        "type": "git",
        "url": "https://github.com/hookun/parse-animation-shorthand"
    },
    "version": "0.1.2",
    "license": "Apache-2.0",
    "author": {
        "name": "Hookun",
        "email": "hookhookun@gmail.com",
        "url": "https://github.com/hookun"
    },
    "main": "cjs/index.js",
    "exports": {
        "import": "./esm/index.js",
        "require": "./cjs/index.js"
    },
    "files": [
        "cjs",
        "esm"
    ],
    "scripts": {
        "build": "run-p build:*",
        "build:esm": "tsc --project tsconfig.esm.json",
        "build:cjs": "tsc --project tsconfig.cjs.json",
        "test": "ava",
        "lint": "eslint --ext .ts src"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "5.35.1",
        "@typescript-eslint/parser": "5.35.1",
        "ava": "4.3.3",
        "eslint": "8.22.0",
        "npm-run-all": "4.1.5",
        "ts-node": "10.9.1",
        "typescript": "4.8.2"
    },
    "ava": {
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    }
}
